- headers = @beta_signups.first.attribute_names - %w[utm_attribution_id updated_at]
- utm_headers = UtmAttribution::FIELDS

.row
  .col.s11.offset-s1
    = link_to 'Download as CSV', beta_signups_download_path(format: 'csv', secret: params[:secret])

%table
  %thead
    %tr
      - headers.each do |header|
        %th.app-typography--body-2= header
      - utm_headers.each do |header|
        %th.app-typography--body-2= header

  %tbody
    - @beta_signups.each do |signup|
      %tr
        - signup.attributes.values_at(*headers).each do |attribute|
          %td.app-typography--body-2= attribute
        - signup.utm_attribution.attributes.values_at(*utm_headers).each do |attribute|
          %td.app-typography--body-2= attribute
